# Database Configuration
DATABASE_URL=postgresql://user:password@postgres-service:5432/ap_database
# or for SQLite: DATABASE_URL=sqlite:///./ap_data.db

# AI/ML Configuration  
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL_NAME=gemini-2.5-flash

# Document Storage
PDF_STORAGE_PATH=/app/documents/pdfs
GENERATED_PDF_STORAGE_PATH=/app/documents/generated

# CORS Configuration (comma-separated URLs)
CORS_ORIGINS=https://your-frontend-domain.com,https://your-staging-domain.com

# Frontend Integration
FRONTEND_URL=https://your-frontend-domain.com

# Processing Configuration
PARALLEL_WORKERS=9
PRICE_TOLERANCE_PERCENT=5.0
QUANTITY_TOLERANCE_PERCENT=0.0

# Application Environment
APP_ENV=production